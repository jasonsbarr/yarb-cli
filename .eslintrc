{
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "script"
  },
  "rules": {
    "prettier/prettier": "warn"
  },
  "plugins": ["prettier"],
  "env": {
    "es6": true,
    "jest": true,
    "node": true
  },
  "ecmaFeatures": {
    "impliedStrict": true
  }
}
